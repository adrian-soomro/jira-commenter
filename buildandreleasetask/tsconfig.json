{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": ["es2019"]
  },
  "exclude": ["node_modules", "**/*.test.ts"],
  "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true
}
